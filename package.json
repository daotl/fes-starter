{
  "name": "@daotl/fes-starter",
  "version": "3.0.0",
  "private": true,
  "packageManager": "pnpm@8.6.0",
  "description": "A fes.js + TypeScript starter template",
  "author": "Nex Zhu <hitnexup@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/daotl/fes-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daotl/fes-starter.git"
  },
  "bugs": {
    "url": "https://github.com/daotl/fes-starter/issues"
  },
  "keywords": [
    "管理端",
    "fes",
    "fast",
    "easy",
    "strong"
  ],
  "scripts": {
    "build": "cross-env RM_TMPDIR=none fes build",
    "prod": "cross-env FES_ENV=prod RM_TMPDIR=none fes build",
    "analyze": "cross-env ANALYZE=1 fes build",
    "dev": "dotenv -e .env.dev -- cross-env FES_APP_DEBUG=true fes dev",
    "test:unit": "fes test:unit",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint \"src/**/*.{css,scss,vue,tsx}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss,vue,tsx}\" --fix",
    "docker-build-prebuilt": "docker build -f docker/Dockerfile.prebuilt -t daotl/fes-starter ."
  },
  "prettier": "@daotl/prettier-config",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@fesjs/fes": "^3.1.1",
    "@fesjs/fes-design": "^0.7.28",
    "@fesjs/plugin-access": "^3.1.0",
    "@fesjs/plugin-layout": "^5.1.2",
    "@fesjs/plugin-pinia": "^3.0.0",
    "@unocss/reset": "^0.52.4",
    "@vueuse/core": "^10.1.2",
    "@vueuse/head": "^1.1.26",
    "element-plus": "^2.3.5",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.3",
    "prism-theme-vars": "^0.2.4",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/runtime": "^7.22.3",
    "@daotl/eslint-config-vue": "^0.3.35",
    "@daotl/prettier-config": "^0.1.4",
    "@daotl/tsconfig": "^0.1.2",
    "@fesjs/builder-vite": "^3.0.1",
    "@iconify-json/mdi": "^1.1.52",
    "@types/node": "^20.2.5",
    "core-js": "^3.30.2",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.41.0",
    "lefthook": "^1.4.1",
    "postcss": "8.4.24",
    "postcss-html": "^1.5.0",
    "postcss-styled-syntax": "^0.4.0",
    "prettier": "^2.8.8",
    "rollup": "^3.23.0",
    "sass": "^1.62.1",
    "stylelint": "^15.6.2",
    "stylelint-config-property-sort-order-smacss": "^9.1.0",
    "stylelint-config-recommended": "^12.0.0",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "typescript": "^5.0.4",
    "unocss": "^0.52.4",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-element-plus": "^0.7.1",
    "unplugin-vue-components": "^0.25.0",
    "vite": "^4.3.9",
    "vite-plugin-inspect": "^0.7.28",
    "vite-plugin-vue-component-preview": "^1.1.6",
    "vite-plugin-vue-inspector": "^3.4.2"
  }
}
