{
  "name": "@daotl/fes-starter",
  "version": "3.0.0",
  "private": true,
  "description": "A fes.js + TypeScript starter template",
  "keywords": [
    "管理端",
    "fes",
    "fast",
    "easy",
    "strong"
  ],
  "license": "MIT",
  "author": "Nex Zhu <hitnexup@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daotl/fes-starter.git"
  },
  "scripts": {
    "build": "cross-env RM_TMPDIR=none fes build",
    "prod": "cross-env FES_ENV=prod RM_TMPDIR=none fes build",
    "analyze": "cross-env ANALYZE=1 fes build",
    "dev": "cross-env FES_APP_DEBUG=true fes dev",
    "test:unit": "fes test:unit",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint \"**/*.{css,scss,vue,tsx}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss,vue,tsx}\" --fix",
    "prepare": "husky install"
  },
  "prettier": "@daotl/prettier-config",
  "bugs": {
    "url": "https://github.com/daotl/fes-starter/issues"
  },
  "homepage": "https://github.com/daotl/fes-starter#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@fesjs/fes": "3.0.0-beta.3",
    "@fesjs/fes-design": "^0.5.4",
    "@fesjs/plugin-access": "3.0.0-beta.3",
    "@fesjs/plugin-layout": "5.0.0-beta.3",
    "@fesjs/plugin-pinia": "3.0.0-beta.3",
    "element-plus": "^2.2.1",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.14",
    "prism-theme-vars": "^0.2.3",
    "vue": "^3.2.35",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/runtime": "^7.18.0",
    "@daotl/eslint-config-vue": "0.3.25",
    "@daotl/prettier-config": "^0.1.3",
    "@daotl/tsconfig": "^0.0.4",
    "@fesjs/builder-vite": "3.0.0-beta.6",
    "@iconify-json/mdi": "^1.1.16",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/node": "^17.0.35",
    "@unocss/preset-attributify": "^0.34.0",
    "@unocss/preset-icons": "^0.34.0",
    "@unocss/preset-typography": "^0.34.0",
    "@unocss/preset-web-fonts": "^0.34.0",
    "@unocss/reset": "^0.34.0",
    "@vue/compiler-sfc": "^3.2.35",
    "core-js": "^3.22.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.16.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "pnpm": "^7.1.3",
    "postcss": "^8.4.14",
    "postcss-html": "^1.4.1",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.6.2",
    "rollup": "^2.74.1",
    "sass": "^1.52.1",
    "stylelint": "^14.8.3",
    "stylelint-config-property-sort-order-smacss": "^9.0.0",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "typescript": "^4.6.4",
    "unocss": "^0.34.0",
    "unplugin-auto-import": "^0.7.2",
    "unplugin-element-plus": "^0.4.0",
    "unplugin-vue-components": "^0.19.5",
    "vite": "^2.9.9"
  }
}
