{
  "name": "@daotl/fes-starter",
  "version": "3.0.0",
  "private": true,
  "packageManager": "pnpm@8.6.0",
  "description": "A fes.js + TypeScript starter template",
  "author": "Nex Zhu <hitnexup@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/daotl/fes-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daotl/fes-starter.git"
  },
  "bugs": {
    "url": "https://github.com/daotl/fes-starter/issues"
  },
  "keywords": [
    "管理端",
    "fes",
    "fast",
    "easy",
    "strong"
  ],
  "scripts": {
    "build": "cross-env RM_TMPDIR=none fes build",
    "prod": "cross-env FES_ENV=prod RM_TMPDIR=none fes build",
    "analyze": "cross-env ANALYZE=1 fes build",
    "dev": "dotenv -e .env.dev -- cross-env FES_APP_DEBUG=true fes dev",
    "test:unit": "fes test:unit",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint \"src/**/*.{css,scss,vue,tsx}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss,vue,tsx}\" --fix",
    "docker-build-prebuilt": "docker build -f docker/Dockerfile.prebuilt -t daotl/fes-starter ."
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@fesjs/fes": "^3.1.3",
    "@fesjs/fes-design": "^0.8.7",
    "@fesjs/plugin-access": "^3.1.2",
    "@fesjs/plugin-layout": "^5.1.6",
    "@fesjs/plugin-pinia": "^3.0.0",
    "@unocss/reset": "^0.55.7",
    "@vueuse/core": "^10.4.1",
    "@vueuse/head": "^1.3.1",
    "element-plus": "^2.3.12",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.6",
    "prism-theme-vars": "^0.2.4",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.17",
    "@babel/runtime": "^7.22.15",
    "@daotl/eslint-config-vue": "^1.0.0-alpha.6",
    "@daotl/tsconfig": "^0.1.2",
    "@fesjs/builder-vite": "^3.0.2",
    "@iconify-json/mdi": "^1.1.54",
    "@types/node": "^20.6.0",
    "core-js": "^3.32.2",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.49.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "lefthook": "^1.4.10",
    "postcss": "8.4.29",
    "postcss-html": "^1.5.0",
    "postcss-styled-syntax": "^0.4.0",
    "rollup": "^3.29.1",
    "sass": "^1.66.1",
    "stylelint": "^15.10.3",
    "stylelint-config-property-sort-order-smacss": "^9.1.0",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-recommended-scss": "^13.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "typescript": "^5.2.2",
    "unocss": "^0.55.7",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.9",
    "vite-plugin-inspect": "^0.7.38",
    "vite-plugin-vue-component-preview": "^1.1.6",
    "vite-plugin-vue-inspector": "^3.7.1"
  }
}
