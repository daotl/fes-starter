{
  "name": "@daotl/fes-starter",
  "version": "3.0.0",
  "private": true,
  "packageManager": "pnpm@7.16.0",
  "description": "A fes.js + TypeScript starter template",
  "author": "Nex Zhu <hitnexup@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/daotl/fes-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daotl/fes-starter.git"
  },
  "bugs": {
    "url": "https://github.com/daotl/fes-starter/issues"
  },
  "keywords": [
    "管理端",
    "fes",
    "fast",
    "easy",
    "strong"
  ],
  "scripts": {
    "build": "cross-env RM_TMPDIR=none fes build",
    "prod": "cross-env FES_ENV=prod RM_TMPDIR=none fes build",
    "analyze": "cross-env ANALYZE=1 fes build",
    "dev": "cross-env FES_APP_DEBUG=true fes dev",
    "test:unit": "fes test:unit",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint \"**/*.{css,scss,vue,tsx}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss,vue,tsx}\" --fix",
    "prepare": "husky install"
  },
  "prettier": "@daotl/prettier-config",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@fesjs/fes": "3.0.0-rc.1",
    "@fesjs/fes-design": "^0.7.9",
    "@fesjs/plugin-access": "3.0.0-rc.1",
    "@fesjs/plugin-layout": "5.0.0-rc.1",
    "@fesjs/plugin-pinia": "3.0.0-rc.1",
    "@vueuse/core": "^9.5.0",
    "@vueuse/head": "^1.0.16",
    "element-plus": "^2.2.21",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.23",
    "prism-theme-vars": "^0.2.4",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/runtime": "^7.20.1",
    "@daotl/eslint-config-vue": "0.3.32",
    "@daotl/prettier-config": "^0.1.3",
    "@daotl/tsconfig": "^0.1.2",
    "@fesjs/builder-vite": "3.0.0-rc.1",
    "@iconify-json/mdi": "^1.1.34",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/node": "^18.11.9",
    "@unocss/preset-attributify": "^0.46.5",
    "@unocss/preset-icons": "^0.46.5",
    "@unocss/preset-typography": "^0.46.5",
    "@unocss/preset-web-fonts": "^0.46.5",
    "@unocss/reset": "^0.46.5",
    "@vue/compiler-sfc": "^3.2.45",
    "core-js": "^3.26.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.27.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "pnpm": "^7.16.1",
    "postcss": "^8.4.19",
    "postcss-html": "^1.5.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.7.1",
    "rollup": "^3.3.0",
    "sass": "^1.56.1",
    "stylelint": "^14.15.0",
    "stylelint-config-property-sort-order-smacss": "^9.0.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "typescript": "^4.9.3",
    "unocss": "^0.46.5",
    "unplugin-auto-import": "^0.11.4",
    "unplugin-element-plus": "^0.4.1",
    "unplugin-vue-components": "^0.22.9",
    "vite": "^3.2.4"
  }
}
