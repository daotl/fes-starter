{
  "name": "@daotl/fes-starter",
  "version": "3.0.0",
  "private": true,
  "packageManager": "pnpm@7.29.1",
  "description": "A fes.js + TypeScript starter template",
  "author": "Nex Zhu <hitnexup@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/daotl/fes-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daotl/fes-starter.git"
  },
  "bugs": {
    "url": "https://github.com/daotl/fes-starter/issues"
  },
  "keywords": [
    "管理端",
    "fes",
    "fast",
    "easy",
    "strong"
  ],
  "scripts": {
    "build": "cross-env RM_TMPDIR=none fes build",
    "prod": "cross-env FES_ENV=prod RM_TMPDIR=none fes build",
    "analyze": "cross-env ANALYZE=1 fes build",
    "dev": "cross-env FES_APP_DEBUG=true fes dev",
    "test:unit": "fes test:unit",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint \"src/**/*.{css,scss,vue,tsx}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss,vue,tsx}\" --fix"
  },
  "prettier": "@daotl/prettier-config",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@fesjs/fes": "^3.0.0",
    "@fesjs/fes-design": "^0.7.23",
    "@fesjs/plugin-access": "^3.0.0",
    "@fesjs/plugin-layout": "^5.0.0",
    "@fesjs/plugin-pinia": "^3.0.0",
    "@vueuse/core": "^9.13.0",
    "@vueuse/head": "^1.1.23",
    "element-plus": "^2.3.2",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.33",
    "prism-theme-vars": "^0.2.4",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/runtime": "^7.21.0",
    "@daotl/eslint-config-vue": "^0.3.35",
    "@daotl/prettier-config": "^0.1.4",
    "@daotl/tsconfig": "^0.1.2",
    "@fesjs/builder-vite": "^3.0.1",
    "@iconify-json/mdi": "^1.1.50",
    "@types/node": "^18.15.11",
    "@unocss/preset-attributify": "^0.50.6",
    "@unocss/preset-icons": "^0.50.6",
    "@unocss/preset-typography": "^0.50.6",
    "@unocss/preset-web-fonts": "^0.50.6",
    "@unocss/reset": "^0.50.6",
    "@vue/compiler-sfc": "^3.2.47",
    "core-js": "^3.29.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.37.0",
    "lefthook": "^1.3.8",
    "pnpm": "^8.1.0",
    "postcss": "8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-styled-syntax": "^0.4.0",
    "prettier": "^2.8.7",
    "rollup": "^3.20.2",
    "sass": "^1.60.0",
    "stylelint": "^15.4.0",
    "stylelint-config-property-sort-order-smacss": "^9.1.0",
    "stylelint-config-recommended": "^11.0.0",
    "stylelint-config-recommended-scss": "^9.0.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "typescript": "^5.0.3",
    "unocss": "^0.50.6",
    "unplugin-auto-import": "^0.15.2",
    "unplugin-element-plus": "^0.7.0",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.2.1",
    "vite-plugin-inspect": "^0.7.18",
    "vite-plugin-vue-component-preview": "^1.1.5",
    "vite-plugin-vue-inspector": "^3.4.0"
  }
}
