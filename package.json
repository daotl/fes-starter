{
  "name": "@daotl/fes-starter",
  "version": "3.0.0",
  "private": true,
  "packageManager": "pnpm@9.1.4",
  "description": "A fes.js + TypeScript starter template",
  "author": "Nex Zhu <hitnexup@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/daotl/fes-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daotl/fes-starter.git"
  },
  "bugs": {
    "url": "https://github.com/daotl/fes-starter/issues"
  },
  "keywords": [
    "管理端",
    "fes",
    "fast",
    "easy",
    "strong"
  ],
  "scripts": {
    "build": "cross-env RM_TMPDIR=none fes build",
    "prod": "cross-env FES_ENV=prod RM_TMPDIR=none fes build",
    "analyze": "cross-env ANALYZE=1 fes build",
    "dev": "dotenv -e .env.dev -- cross-env FES_APP_DEBUG=true fes dev",
    "test:unit": "fes test:unit",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint \"src/**/*.{css,scss,vue,tsx}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss,vue,tsx}\" --fix",
    "docker-build-prebuilt": "docker build -f docker/Dockerfile.prebuilt -t daotl/fes-starter ."
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@fesjs/fes": "^3.1.4",
    "@fesjs/fes-design": "^0.8.11",
    "@fesjs/plugin-access": "^3.1.3",
    "@fesjs/plugin-layout": "^5.1.7",
    "@fesjs/plugin-pinia": "^3.0.1",
    "@unocss/reset": "^0.57.0",
    "@vueuse/core": "^10.10.0",
    "@vueuse/head": "^2.0.0",
    "element-plus": "^2.7.3",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "prism-theme-vars": "^0.2.5",
    "vue": "^3.4.27",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/runtime": "^7.24.6",
    "@daotl/eslint-config": "^2.1.0",
    "@daotl/tsconfig": "^0.1.2",
    "@fesjs/builder-vite": "^4.0.2",
    "@iconify-json/mdi": "^1.1.66",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@types/node": "^20.12.12",
    "@unocss/eslint-plugin": "^0.57.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue-macros/volar": "^0.21.1",
    "core-js": "^3.37.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.3.0",
    "eslint-plugin-format": "^0.1.1",
    "lefthook": "^1.6.13",
    "postcss": "8.4.38",
    "postcss-html": "^1.7.0",
    "postcss-styled-syntax": "^0.6.4",
    "rollup": "^4.18.0",
    "sass": "^1.77.2",
    "stylelint": "^16.6.0",
    "stylelint-config-property-sort-order-smacss": "^10.0.0",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "typescript": "^5.4.5",
    "unocss": "^0.57.0",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.27.0",
    "unplugin-vue-macros": "^2.9.3",
    "vite": "^5.2.11",
    "vite-plugin-inspect": "^0.8.4",
    "vite-plugin-vue-devtools": "7.2.1",
    "vite-plugin-vue-inspector": "^5.1.2"
  }
}
