{
  "name": "@daotl/fes-starter",
  "version": "3.0.0",
  "private": true,
  "packageManager": "pnpm@7.17.0",
  "description": "A fes.js + TypeScript starter template",
  "author": "Nex Zhu <hitnexup@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/daotl/fes-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daotl/fes-starter.git"
  },
  "bugs": {
    "url": "https://github.com/daotl/fes-starter/issues"
  },
  "keywords": [
    "管理端",
    "fes",
    "fast",
    "easy",
    "strong"
  ],
  "scripts": {
    "build": "cross-env RM_TMPDIR=none fes build",
    "prod": "cross-env FES_ENV=prod RM_TMPDIR=none fes build",
    "analyze": "cross-env ANALYZE=1 fes build",
    "dev": "cross-env FES_APP_DEBUG=true fes dev",
    "test:unit": "fes test:unit",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint \"src/**/*.{css,scss,vue,tsx}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss,vue,tsx}\" --fix"
  },
  "prettier": "@daotl/prettier-config",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@fesjs/fes": "^3.0.0-rc.4",
    "@fesjs/fes-design": "^0.7.19",
    "@fesjs/plugin-access": "^3.0.0-rc.3",
    "@fesjs/plugin-layout": "^5.0.0-rc.2",
    "@fesjs/plugin-pinia": "^3.0.0-rc.3",
    "@vueuse/core": "^9.12.0",
    "@vueuse/head": "^1.0.25",
    "element-plus": "^2.2.30",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.30",
    "prism-theme-vars": "^0.2.4",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/runtime": "^7.20.13",
    "@daotl/eslint-config-vue": "^0.3.32",
    "@daotl/prettier-config": "^0.1.4",
    "@daotl/tsconfig": "^0.1.2",
    "@fesjs/builder-vite": "^3.0.0-rc.1",
    "@iconify-json/mdi": "^1.1.47",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/node": "^18.13.0",
    "@unocss/preset-attributify": "^0.49.4",
    "@unocss/preset-icons": "^0.49.4",
    "@unocss/preset-typography": "^0.49.4",
    "@unocss/preset-web-fonts": "^0.49.4",
    "@unocss/reset": "^0.49.4",
    "@vue/compiler-sfc": "^3.2.47",
    "core-js": "^3.28.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.34.0",
    "lefthook": "^1.2.9",
    "pnpm": "^7.27.0",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.4",
    "rollup": "^3.15.0",
    "sass": "^1.58.1",
    "stylelint": "^15.1.0",
    "stylelint-config-property-sort-order-smacss": "^9.0.0",
    "stylelint-config-recommended": "^10.0.1",
    "stylelint-config-recommended-scss": "^9.0.1",
    "stylelint-config-recommended-vue": "^1.4.0",
    "typescript": "^4.9.5",
    "unocss": "^0.49.4",
    "unplugin-auto-import": "^0.14.2",
    "unplugin-element-plus": "^0.6.0",
    "unplugin-vue-components": "^0.23.0",
    "vite": "^4.1.1",
    "vite-plugin-inspect": "^0.7.15",
    "vite-plugin-vue-component-preview": "^1.0.2",
    "vite-plugin-vue-inspector": "^3.3.2"
  }
