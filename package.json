{
  "name": "@daotl/fes-starter",
  "version": "3.0.0",
  "private": true,
  "packageManager": "pnpm@8.9.2",
  "description": "A fes.js + TypeScript starter template",
  "author": "Nex Zhu <hitnexup@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/daotl/fes-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daotl/fes-starter.git"
  },
  "bugs": {
    "url": "https://github.com/daotl/fes-starter/issues"
  },
  "keywords": [
    "管理端",
    "fes",
    "fast",
    "easy",
    "strong"
  ],
  "scripts": {
    "build": "cross-env RM_TMPDIR=none fes build",
    "prod": "cross-env FES_ENV=prod RM_TMPDIR=none fes build",
    "analyze": "cross-env ANALYZE=1 fes build",
    "dev": "dotenv -e .env.dev -- cross-env FES_APP_DEBUG=true fes dev",
    "test:unit": "fes test:unit",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint \"src/**/*.{css,scss,vue,tsx}\"",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss,vue,tsx}\" --fix",
    "docker-build-prebuilt": "docker build -f docker/Dockerfile.prebuilt -t daotl/fes-starter ."
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@fesjs/fes": "^3.1.4",
    "@fesjs/fes-design": "^0.8.11",
    "@fesjs/plugin-access": "^3.1.3",
    "@fesjs/plugin-layout": "^5.1.7",
    "@fesjs/plugin-pinia": "^3.0.1",
    "@unocss/reset": "^0.57.0",
    "@vueuse/core": "^10.5.0",
    "@vueuse/head": "^2.0.0",
    "element-plus": "^2.4.1",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "prism-theme-vars": "^0.2.4",
    "vue": "^3.3.6",
    "vue-i18n": "^9.5.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/runtime": "^7.23.2",
    "@daotl/eslint-config": "^2.0.0-beta.0",
    "@daotl/tsconfig": "^0.1.2",
    "@fesjs/builder-vite": "^3.0.3",
    "@iconify-json/mdi": "^1.1.54",
    "@intlify/unplugin-vue-i18n": "^1.4.0",
    "@types/node": "^20.8.8",
    "@unocss/eslint-plugin": "^0.57.0",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue-macros/volar": "^0.17.0",
    "core-js": "^3.33.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.52.0",
    "lefthook": "^1.5.2",
    "postcss": "8.4.31",
    "postcss-html": "^1.5.0",
    "postcss-styled-syntax": "^0.5.0",
    "rollup": "^4.1.4",
    "sass": "^1.69.4",
    "stylelint": "^15.11.0",
    "stylelint-config-property-sort-order-smacss": "^9.1.0",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-recommended-scss": "^13.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "typescript": "^5.2.2",
    "unocss": "^0.57.0",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.25.2",
    "unplugin-vue-macros": "^2.6.1",
    "vite": "^4.5.0",
    "vite-plugin-inspect": "^0.7.40",
    "vite-plugin-vue-devtools": "1.0.0-rc.5",
    "vite-plugin-vue-inspector": "^4.0.0"
  }
}
