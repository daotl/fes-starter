{
  "name": "@daotl/fes-starter",
  "version": "2.0.0",
  "description": "A fes.js + TypeScript starter template",
  "private": true,
  "scripts": {
    "build": "fes build",
    "prod": "cross-env FES_ENV=prod fes build",
    "analyze": "cross-env ANALYZE=1 fes build",
    "dev": "fes dev",
    "test:unit": "fes test:unit",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint '**/*.{js,ts,tsx,vue}'",
    "eslint:fix": "eslint '**/*.{js,ts,tsx,vue}' --fix",
    "stylelint": "stylelint 'src/**/*.{css,scss,vue,tsx}'",
    "stylelint:fix": "stylelint 'src/**/*.{css,scss,vue,tsx}' --fix",
    "prepare": "husky install"
  },
  "prettier": "@daotl/prettier-config",
  "keywords": [
    "管理端",
    "fes",
    "fast",
    "easy",
    "strong"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daotl/fes-starter.git"
  },
  "author": "Nex Zhu <hitnexup@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daotl/fes-starter/issues"
  },
  "homepage": "https://github.com/daotl/fes-starter#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@fesjs/fes": "^2.0.21",
    "@fesjs/fes-design": "^0.3.5",
    "@fesjs/plugin-access": "^2.0.4",
    "@fesjs/plugin-enums": "^2.0.2",
    "@fesjs/plugin-layout": "^4.0.6",
    "core-js": "^3.21.1",
    "element-plus": "^2.0.5",
    "prism-theme-vars": "^0.2.2",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/runtime": "^7.17.2",
    "@daotl/eslint-config-vue": "0.2.5",
    "@daotl/prettier-config": "^0.1.2",
    "@daotl/tsconfig": "^0.0.4",
    "@fesjs/plugin-sass": "^2.0.0",
    "@iconify-json/mdi": "^1.1.1",
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.21",
    "@unocss/preset-attributify": "^0.27.6",
    "@unocss/preset-icons": "^0.27.6",
    "@unocss/preset-typography": "^0.27.6",
    "@unocss/preset-web-fonts": "^0.27.6",
    "@unocss/reset": "^0.27.6",
    "@unocss/webpack": "^0.27.6",
    "@vue/compiler-sfc": "^3.2.31",
    "core-js": "^3.21.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.10.0",
    "eslint-webpack-plugin": "^3.1.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "pinia": "^2.0.11",
    "pnpm": "^6.32.3",
    "postcss": "^8.4.8",
    "postcss-html": "^1.3.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.5.1",
    "stylelint": "^14.5.3",
    "stylelint-config-property-sort-order-smacss": "^9.0.0",
    "stylelint-config-recommended-scss": "^5.0.2",
    "stylelint-config-recommended-vue": "^1.3.0",
    "stylelint-webpack-plugin": "^3.1.1",
    "typescript": "^4.6.2",
    "unocss": "^0.27.6",
    "unplugin-auto-import": "^0.6.1",
    "unplugin-element-plus": "^0.3.1",
    "unplugin-vue-components": "^0.17.21",
    "webpack": "^5.70.0"
