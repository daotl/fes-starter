{
  "name": "@daotl/fes-starter",
  "version": "2.0.0",
  "private": true,
  "description": "A fes.js + TypeScript starter template",
  "keywords": [
    "管理端",
    "fes",
    "fast",
    "easy",
    "strong"
  ],
  "license": "MIT",
  "author": "Nex Zhu <hitnexup@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daotl/fes-starter.git"
  },
  "scripts": {
    "build": "cross-env RM_TMPDIR=none fes build",
    "prod": "cross-env FES_ENV=prod RM_TMPDIR=none fes build",
    "analyze": "cross-env ANALYZE=1 fes build",
    "dev": "cross-env FES_APP_DEBUG=true fes dev",
    "test:unit": "fes test:unit",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint \"**/*.{css,scss,vue,tsx}\"",
    "stylelint:fix": "stylelint \"**/*.{css,scss,vue,tsx}\" --fix",
    "prepare": "husky install"
  },
  "prettier": "@daotl/prettier-config",
  "bugs": {
    "url": "https://github.com/daotl/fes-starter/issues"
  },
  "homepage": "https://github.com/daotl/fes-starter#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@fesjs/fes": "^2.1.0",
    "@fesjs/fes-design": "^0.5.1",
    "@fesjs/plugin-access": "^2.0.6",
    "@fesjs/plugin-layout": "^4.1.0",
    "@fesjs/plugin-pinia": "^2.0.2",
    "@vicons/material": "^0.12.0",
    "element-plus": "^2.2.0",
    "pinia": "^2.0.14",
    "prism-theme-vars": "^0.2.2",
    "vue": "^3.2.33",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/runtime": "^7.17.9",
    "@daotl/eslint-config-vue": "0.3.23",
    "@daotl/prettier-config": "^0.1.3",
    "@daotl/tsconfig": "^0.0.4",
    "@fesjs/plugin-sass": "^2.0.0",
    "@iconify-json/mdi": "^1.1.12",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/node": "^17.0.33",
    "@unocss/preset-attributify": "^0.33.4",
    "@unocss/preset-icons": "^0.33.4",
    "@unocss/preset-typography": "^0.33.4",
    "@unocss/preset-web-fonts": "^0.33.4",
    "@unocss/reset": "^0.33.4",
    "@unocss/webpack": "^0.33.4",
    "@vicons/utils": "^0.1.4",
    "@vue/compiler-sfc": "^3.2.33",
    "core-js": "^3.22.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "eslint-webpack-plugin": "^3.1.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "pnpm": "^7.1.0",
    "postcss": "^8.4.13",
    "postcss-html": "^1.4.1",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.6.2",
    "stylelint": "^14.8.2",
    "stylelint-config-property-sort-order-smacss": "^9.0.0",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-webpack-plugin": "^3.2.0",
    "typescript": "^4.6.4",
    "unocss": "^0.33.4",
    "unplugin-auto-import": "^0.7.1",
    "unplugin-element-plus": "^0.4.0",
    "unplugin-vue-components": "^0.19.5",
    "webpack": "^5.72.1"
  }
}
