{
  "extends": "@daotl/tsconfig/vue.json",
  "compilerOptions": {
    "target": "es2020",
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"],
      "@@/*": ["src/.fes/*", "src/.fes-production/*"]
    },
    "types": ["node", "jest", "element-plus/global"],
    "outDir": "build/dist",
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules",
    "build",
    "dist",
    "src/.fes",
    "src/.fes-production"
  ]
}
